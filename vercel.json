{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(script\\.js|advanced-humanizer\\.js|styles\\.css)",
      "headers": {
        "cache-control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "pragma": "no-cache"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js",
      "headers": {
        "cache-control": "no-store, no-cache, must-revalidate, proxy-revalidate"
      }
    }
  ]
}
