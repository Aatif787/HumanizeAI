{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(script\\.js|styles\\.css)",
      "headers": {
        "cache-control": "no-cache, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "/(video\\.mp4)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate, proxy-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      }
    }
  ]
}
