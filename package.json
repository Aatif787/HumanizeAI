{
  "name": "ai-text-humanizer-fullstack",
  "version": "1.0.0",
  "description": "Full-stack AI text humanizer with advanced detection evasion",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "npm run vercel-build",
    "vercel-build": "npm install && pip install -r requirements.txt && npm run build:assets && echo 'Vercel build complete - v1.1.8-force'",
    "build:assets": "echo 'Building assets...' && node -e \"console.log('Asset build complete - ' + new Date().toISOString())\"",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "docx": "^8.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.5.0",
    "node-cron": "^3.0.3",
    "pdf-lib": "^1.17.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "typescript-eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=20.0.0 <22.0.0",
    "npm": ">=8.0.0 <11.0.0"
  },
  "keywords": [
    "ai",
    "text-humanization",
    "content-generation",
    "nlp",
    "fullstack"
  ],
  "author": "AI Text Humanizer Team",
  "license": "MIT"
}
